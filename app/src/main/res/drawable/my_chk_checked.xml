<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"

    >
    <item>
        <shape
            android:shape="oval"
            >
            <solid
                android:color="@color/colorAccent"
                android:width="1dp"
                />
            <stroke
                android:width="1dp"
                android:color="@color/colorAccent"
                />
        </shape>

    </item>

    <item
        android:bottom="4dp"
        android:top="4dp"
        android:right="5dp"
        android:left="4dp"
        android:width="15dp"
        android:height="15dp"
        >
        <bitmap
            android:tint="@android:color/white"
            android:src="@drawable/check_symbol"
            />
    </item>



</layer-list>
